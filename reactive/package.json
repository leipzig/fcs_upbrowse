    "build.dev.watch": "gulp build.dev.watch --color",
    "serve.dev": "gulp serve.dev --color --off-inj-msg",
    "build.prod": "gulp build.prod --color --env prod --off-inj-msg",
    "start": "gulp serve.prod --color --env prod --off-inj-msg",
    "build.html": "gulp build.html --color --html",
    "serve.html": "gulp serve.html.dev --color --off-inj-msg",
    "build.html.prod": "gulp build.html.prod --color --env prod",
    "serve.html.prod": "gulp serve.html.prod --color --env prod --off-inj-msg",
