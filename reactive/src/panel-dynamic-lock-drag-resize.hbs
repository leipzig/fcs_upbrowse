                                    <div class="item" data-value="WI">Wisconsin</div>
                                    <div class="item" data-value="WY">Wyoming</div>
                                </div>
                            </div>
                            {{#embed "panel-icons" slide="1"}}{{/embed}}
                        </div>
                        <div class="panel-body">
                            <p class="mb-0 panel-body-p">This is a panel with no constraints</p>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section data-gs-height="30" class="col-sm-12 col-md-12 col-lg-6 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark">
                        <div class="panel-header panel-header-p grid-stack-handle bgc-white-dark panel-header-sm">
                            <h2 class="pull-left">
                                Panel
                            </h2>
                            <div class="hidden-xs-down panel-input pull-left hidden-xs-down">
                                <input class="form-control form-control-sm" type="text" placeholder="Search">
                                <a href="" class="clear-style"><i class="fa fa-search"></i></a>
                            </div>
                            {{#embed "panel-icons" slide="1"}}{{/embed}}
                        </div>
                        <div class="panel-body panel-body-p">
                            <p class="mb-0">This a panel with no constraints</p>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section data-gs-no-resize="true" data-gs-height="30" class="col-sm-12 col-md-12 col-lg-6 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark">
                        <div class="panel-header panel-header-p grid-stack-handle bgc-white-dark panel-header-sm">
                            <h2 class="pull-left">
                                Isn't Resizable
                            </h2>
                            <div class="hidden-xs-down panel-input pull-left">
                                <input class="form-control form-control-sm" type="text" placeholder="Search">
                                <a href="" class="clear-style"><i class="fa fa-search"></i></a>
                            </div>
                            {{#embed "panel-icons" slide="1"}}{{/embed}}
                        </div>
                        <div class="panel-body panel-body-p">
                            <p class="">This panel could not be resized. Other panels could change its position.</p>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section data-gs-height="30" class="col-sm-12 col-md-12 col-lg-12 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark">
                        <div class="panel-header panel-header-p grid-stack-handle bgc-white-dark panel-header-sm">
                            <h2 class="pull-left">
