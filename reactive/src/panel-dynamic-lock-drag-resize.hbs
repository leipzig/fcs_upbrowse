{{#extend "layout-1" current='panel-dynamic-lock-drag-resize'}}
{{! append html code into the content block declared in the layout partial }}
    {{#content "content" mode="append"}}
        <div class="container-fluid content-wrap">
            <div class="row panel-grid grid-stack">
                <section class="col-sm-12 col-md-12 col-lg-12 panel-wrap panel-grid-item grid-stack-item" data-gs-locked="true" data-gs-no-resize="true" data-gs-no-move="true">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark pb-3">
                        <div class="panel-header clearfix  bgc-white-darkest panel-header-lg">
                            <h2 class="pull-left">
                                Drag & Resize
                            </h2>
                        </div>
                        <div class="panel-body panel-body-p">
                            <p>Draggability and Resizability are controllable through HTML <code>data-gs-*</code> attributes</p>
                            <p><code>data-gs-no-resize, data-gs-no-move, data-gs-locked</code> attributes are supported to disable resizing, dragging and for locking panels respectively</p>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section data-gs-locked="Yes"  data-gs-height="40" class="col-sm-12 col-md-12 col-lg-6 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark">
                        <div class="panel-header panel-header-p grid-stack-handle bgc-white-darkest panel-header-sm">
                            <h2 class="pull-left">
                                Locked Panel
                            </h2>
                            <div class="hidden-xs-down ui custom-select-dropdown select-dropdown lh-6">
                                <div class="text">Do something</div>
                                <i class="select-dropdown icon"></i>
                                <div class="menu">
                                    <div class="item">New</div>
                                    <div class="item">
                                        <span class="description">ctrl + o</span>
                                        Open...
                                    </div>
                                    <div class="item">
                                        <span class="description">ctrl + s</span>
                                        Save as...
                                    </div>
                                    <div class="item">
                                        <span class="description">ctrl + r</span>
                                        Rename
                                    </div>
                                    <div class="item">Make a copy</div>
                                    <div class="item">
                                        <i class="fa fa-folder icon"></i>
                                        Move to folder
                                    </div>
                                    <div class="item">
                                        <i class="fa fa-trash icon"></i>
                                        Move to trash
                                    </div>
                                    <div class="divider"></div>
                                    <div class="item">Download As...</div>
                                    <div class="item">
                                        <i class="select-dropdown icon"></i>
                                        Publish To Web
                                        <div class="menu">
                                            <div class="item">Google Docs</div>
                                            <div class="item">Google Drive</div>
                                            <div class="item">Dropbox</div>
                                            <div class="item">Adobe Creative Cloud</div>
                                            <div class="item">Private FTP</div>
                                            <div class="item">Another Service...</div>
                                        </div>
                                    </div>
                                    <div class="item">E-mail Collaborators</div>
                                </div>
                            </div>
                            {{#embed "panel-icons" slide="1" class="bgc-white-darkest bgc-active-dark-trans"}}{{/embed}}
                        </div>
                        <div class="panel-body">
                            <p class="mb-0 panel-body-p">This panel is locked in its position. Other panels could not move it during dragging or resizing</p>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section data-gs-no-move="Yes" data-gs-height="30" class="col-sm-12 col-md-12 col-lg-6 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark">
                        <div class="panel-header panel-header-p grid-stack-handle bgc-white-dark panel-header-sm">
                            <h2 class="pull-left">
                                Is't Draggable
                            </h2>
                            <div class="dropdown pull-left hidden-xs-down">
                                <a href="" class="panel-header-link" id="panelDropdownMenu1" data-toggle="dropdown"
                                   aria-haspopup="true" aria-expanded="false">
                                    <span>Drop Down</span><i class="fa fa-angle-down icon-ml"></i>
                                </a>
                                <div class="dropdown-menu lh-5" aria-labelledby="panelDropdownMenu1">
                                    <div class="dropdown-menu-content">
                                        <a class="dropdown-item" href="">Action</a>
                                        <a class="dropdown-item" href="">Another action</a>
                                        <a class="dropdown-item" href="">Something else here</a>
                                    </div>
                                </div>
                            </div>
                            {{#embed "panel-icons" slide="1"}}{{/embed}}
                        </div>
                        <div class="panel-body panel-body-p">
                            <p>This panel could not be dragged</p>
                            <p class="mb-0">Other panels could change its position</p>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section data-gs-height="40" class="col-sm-12 col-md-12 col-lg-6 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark">
                        <div class="panel-header panel-header-p grid-stack-handle bgc-white-dark panel-header-sm">
                            <h2 class="pull-left">
                                Panel
                            </h2>
