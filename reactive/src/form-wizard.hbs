{{#extend "layout-1" current="form-wizard"}}
{{! append html code into the content block declared in the layout partial }}
    {{#content "content" mode="append"}}
        <div class="container-fluid content-wrap">
            <div class="row panel-grid" id="panel-grid">
                <section class="col-sm-12 col-md-12 col-xl-12 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark b-shadow-none">
                        <div class="panel-header clearfix  panel-header-p bgc-white-darkest panel-header-sm">
                            <h2 class="pull-left">
                                Navigation by Buttons
                            </h2>
                            {{#embed "panel-icons" slide="1" class="bgc-white-darkest"}}{{/embed}}
                        </div>
                        <div class="panel-body panel-body-p">
                            <div class="step-animate wizard-buttons">
                                <h3>Keyboard</h3>
                                <section class="px-2">
                                    <p class="mb-0">Try the keyboard navigation by clicking arrow left or right!</p>
                                    <p class="mb-0">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Eos, sapiente.</p>
                                </section>
                                <h3>Effects</h3>
                                <section class="px-2">
                                    <p class="mb-0">Wonderful transition effects.</p>
                                </section>
                                <h3>Pager</h3>
                                <section class="px-2">
                                    <p class="mb-0">The next and previous buttons help you to navigate through your content.</p>
                                </section>
                            </div>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section class="col-sm-12 col-md-12 col-xl-12 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark b-shadow-none">
