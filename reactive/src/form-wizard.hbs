                                <h3>Keyboard</h3>
                                <section class="px-2">
                                    <p class="mb-0">Navigate by clicking on the steps</p>
                                    <p class="mb-0">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Eos, sapiente.</p>
                                </section>
                                <h3>Effects</h3>
                                <section class="px-2">
                                    <p class="mb-0">Wonderful transition effects.</p>
                                </section>
                                <h3>Pager</h3>
                                <section class="px-2">
                                    <p class="mb-0">The next and previous buttons help you to navigate through your content.</p>
                                </section>
                            </div>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
                <section class="col-sm-12 col-md-12 col-xl-12 panel-wrap panel-grid-item grid-stack-item">
                    <!--Start Panel-->
                    <div class="panel bgc-white-dark b-shadow-none">
                        <div class="panel-header clearfix  panel-header-p bgc-white-darkest panel-header-sm">
                            <h2 class="pull-left">
                                With Form
                            </h2>
                            {{#embed "panel-icons" slide="1" class="bgc-white-darkest"}}{{/embed}}
                        </div>
                        <div class="panel-body panel-body-p">
                            <form class="form-step-validation">
                                <div class="step-animate wizard-navigation">
                                    <h3>Keyboard</h3>
                                    <section class="px-2">
                                        <div class="form-group">
                                            <label for="exampleInputEmail1">Email address</label>
                                            <input type="email" class="form-control required" id="exampleInputEmail1" aria-describedby="emailHelp1" placeholder="Enter email" name="email">
                                            <small id="emailHelp1" class="form-text text-muted">We'll never share your email with
                                                anyone else.
                                            </small>
                                        </div>
                                    </section>
                                    <h3>Effects</h3>
                                    <section class="px-2">
                                        <div class="form-group">
                                            <label for="exampleInputPassword1">Password</label>
                                            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" name="password">
                                        </div>
                                    </section>
                                    <h3>Pager</h3>
                                    <section class="px-2">
                                        <div class="form-check form-group">
                                            <label class="custom-control custom-checkbox ui checkbox d-block">
                                                <input type="checkbox" class="form-control custom-control-input" name="checked">
                                                <span class="custom-control-indicator"></span>
                                                <span class="custom-control-description ml-2">Agree to the terms and conditions</span>
                                            </label>
                                        </div>
                                    </section>
                                </div>
                            </form>
                        </div>
                    </div>
                    <!--End Panel-->
                </section>
