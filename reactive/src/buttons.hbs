                                <p class="fs-6-plus mb-2">Color Buttons with <code>.btn-default .btn-primary .btn-success .btn-info .btn-warning .btn-danger</code> classes</p>
                                <button class="btn mb-1 btn-default">Default</button>
                                <button class="btn mb-1 btn-primary">Primary</button>
                                <button class="btn mb-1 btn-success">Success</button>
                                <button class="btn mb-1 btn-info">Info</button>
                                <button class="btn mb-1 btn-warning">Warning</button>
                                <button class="btn mb-1 btn-danger">Danger</button>
                            </div>
